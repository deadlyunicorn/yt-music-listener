{
  "manifest_version":3,
  "name":"YTMusic Listener",
  "description":"Share what you are currently listening to on YTMusic, to your discord friends!",
  "version":"1.0.0",
  "icons":{
    "16":"icons/YT_listener_16.png",
    "32":"icons/YT_listener_32.png",
    "48":"icons/YT_listener_48.png",
    "128":"icons/YT_listener_128.png"
  },
  "background":{
    "service_worker":"background.js"
  },
  "action":{
    "default_icon":{
    "16":"icons/YT_listener_16.png",
    "32":"icons/YT_listener_32.png",
    "48":"icons/YT_listener_48.png",
    "128":"icons/YT_listener_128.png"
    }
  },
  "permissions":["activeTab","scripting","tabs"],
  "host_permissions": [
    "https://music.youtube.com/*",
    "https://discord.com/*"
  ]

}